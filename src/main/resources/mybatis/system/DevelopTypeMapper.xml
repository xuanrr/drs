<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.project.system.developtype.mapper.DevelopTypeMapper">

    <resultMap type="DevelopType" id="DevelopTypeResult">
        <result property="name"    column="name"    />
        <result property="id"    column="id"    />
        <result property="code"    column="code"    />
        <result property="sort"    column="sort"    />
        <result property="remark"    column="remark"    />
        <result property="available"    column="available"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectDevelopTypeVo">
        select name, id, code, sort, remark, available, create_by, create_time, update_by, update_time from m_develop_type
    </sql>

    <select id="selectDevelopTypeList" parameterType="DevelopType" resultMap="DevelopTypeResult">
        <include refid="selectDevelopTypeVo"/>
        <where>
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="code != null  and code != ''"> and code = #{code}</if>
            <if test="sort != null "> and sort = #{sort}</if>
            <if test="available != null "> and available = #{available}</if>
            <if test="createBy != null  and createBy != ''"> and create_by = #{createBy}</if>
            <if test="createTime != null "> and create_time = #{createTime}</if>
            <if test="updateBy != null  and updateBy != ''"> and update_by = #{updateBy}</if>
            <if test="updateTime != null "> and update_time = #{updateTime}</if>
        </where>
    </select>

    <select id="selectDevelopTypeByName" parameterType="String" resultMap="DevelopTypeResult">
        <include refid="selectDevelopTypeVo"/>
        where name = #{name}
    </select>

    <insert id="insertDevelopType" parameterType="DevelopType">
        insert into m_develop_type
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="name != null and name != ''">name,</if>
            <if test="id != null">id,</if>
            <if test="code != null and code != ''">code,</if>
            <if test="sort != null">sort,</if>
            <if test="remark != null">remark,</if>
            <if test="available != null">available,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="name != null and name != ''">#{name},</if>
            <if test="id != null">#{id},</if>
            <if test="code != null and code != ''">#{code},</if>
            <if test="sort != null">#{sort},</if>
            <if test="remark != null">#{remark},</if>
            <if test="available != null">#{available},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
        </trim>
    </insert>

    <update id="updateDevelopType" parameterType="DevelopType">
        update m_develop_type
        <trim prefix="SET" suffixOverrides=",">
            <if test="id != null">id = #{id},</if>
            <if test="code != null and code != ''">code = #{code},</if>
            <if test="sort != null">sort = #{sort},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="available != null">available = #{available},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where name = #{name}
    </update>

    <delete id="deleteDevelopTypeByName" parameterType="String">
        delete from m_develop_type where name = #{name}
    </delete>

    <delete id="deleteDevelopTypeByNames" parameterType="String">
        delete from m_develop_type where name in
        <foreach item="name" collection="array" open="(" separator="," close=")">
            #{name}
        </foreach>
    </delete>

</mapper>